{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "aloha/twilio": "^3.0",
        "aws/aws-sdk-php-laravel": "^3.1",
        "barryvdh/laravel-snappy": "^0.3.3",
        "cartalyst/stripe": "^2.0",
        "enniel/ami": "^2.1",
        "enniel/laravel-asterisk-notification-channel": "^0.1.1",
        "froiden/laravel-installer": "^1.3",
        "fx3costa/laravelchartjs": "^2.2",
        "giggsey/libphonenumber-for-php": "^8.4",
        "google/recaptcha": "~1.1",
        "h4cc/wkhtmltopdf-amd64": "^0.12.3",
        "ismaeltoe/osms": "^2.0",
        "jeroen-g/laravel-packager": "^1.5",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.2.0",
        "maatwebsite/excel": "~2.1.0",
        "nexmo/client": "^1.1",
        "openpayu/openpayu": "^2.2",
        "ovh/ovh": "^2.0",
        "phpmailer/phpmailer": "^5.2",
        "plivo/plivo-php": "^1.1",
        "srmklive/paypal": "~1.0",
        "symfony/polyfill-iconv": "^1.5",
        "telesign/telesign": "^2.2",
        "twilio/sdk": "^4.0",
        "unicodeveloper/laravel-paystack": "1.0.*",
        "yajra/laravel-datatables-oracle": "^7.5"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database",
            "app/Models"
        ],
        "psr-4": {
            "kashem\\licenseChecker\\": "packages/kashem/licenseChecker/src",
            "App\\": "app/"
        },
      "files": [
        "app/Helpers/helpers.php"
      ]
    },
    "autoload-dev": {
        "psr-4": {
            "kashem\\licenseChecker\\": "packages/kashem/licenseChecker/src",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
